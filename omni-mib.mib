omniEventVoipLinkStatus  NOTIFICATION-TYPE
     OBJECTS {
        omniVoipLinkUp,
        omniVoipChannelName
     }
     STATUS current
     DESCRIPTION
         "VoIP link status change. 0  = Link down, 1 = link up, 255= disabled."
     ::= { omniNotifications 1 }

omniEventVoipRxSeqErrPackets NOTIFICATION-TYPE
     OBJECTS {
        omniVoipRxSequenceErrorPackets
    }
    STATUS current
     DESCRIPTION
         "VoIP Link has too many missing or incorrect sequence numbers"
     ::= { omniNotifications 2 }

omniEventVoipRxTooLatePackets NOTIFICATION-TYPE
     OBJECTS {
        omniVoipRxTooLatePackets
    }
    STATUS current
     DESCRIPTION
         "VoIP Link has too many packets that arrived too late to be played"
     ::= { omniNotifications 3 }

omniEventPushToTalkTimeout  NOTIFICATION-TYPE
     OBJECTS {
         omniRadioPushToTalk
     }
     STATUS current
     DESCRIPTION
         "Radio Push-To-Talk Timeout"
