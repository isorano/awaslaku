OMNI-MIB DEFINITIONS ::= BEGIN

--
--  IMPORTS
--
--  SNMPv2-SMI : RFC2578 (Structure of Management Information Version 2)
--
--  SNMPv2-TC  : RFC2579 (Textual Conventions for SMIv2)
--
--  SNMPv2-CONF: RFC2580 (Conformance Statements for SMIv2)
--
IMPORTS
    OBJECT-TYPE,MODULE-IDENTITY , NOTIFICATION-TYPE,
    enterprises,
    Integer32,Counter32                FROM SNMPv2-SMI
    DisplayString                      FROM SNMPv2-TC
    OBJECT-GROUP,NOTIFICATION-GROUP    FROM SNMPv2-CONF;

--
--  For SNMPv2 the MODULE-IDENTITY must immediately follow the IMPORT construct.
--
--  27053 is the IANA-assigned number for Omnitronics.
--
omnitronics MODULE-IDENTITY
    LAST-UPDATED "202107020000Z"
    ORGANIZATION "Omnitronics"
    CONTACT-INFO
    "
      Omnitronics Pty Ltd
      27 Sarich Court
      Osborne Park
      Perth, Australia"

    DESCRIPTION
            "Omnitronics Enterprise MIBS: Omnitronics Radio interface Products"

    REVISION    "202107020000Z"
    DESCRIPTION
            "Omnitronics MIB file that describes OIDs used by all Omnitronics radio interface products"
    ::= { enterprises 27053 }

--
--  OIDs linked to omnitronics
--
omniNotifications   OBJECT IDENTIFIER ::=  { omnitronics   0 }
omniProducts        OBJECT IDENTIFIER ::=  { omnitronics   1 }
