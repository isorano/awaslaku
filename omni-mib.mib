---
---  Voip Information: omniVoip
---
omniVoipNumberOfChannels OBJECT-TYPE
    SYNTAX Integer32(0..128)
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION "Number of Voip Channels supported by this device"
        ::= { omniVoip 1 }

omniVoipStatusTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF OmniVoipStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Table contains the status of VoIP links"
    ::=  { omniVoip 2 }

omniVoipStatusEntry OBJECT-TYPE
        SYNTAX      OmniVoipStatusEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "Row describes status of a Voip connection"
        INDEX   { omniVoipChannelIndex }
        ::= { omniVoipStatusTable 1 }

OmniVoipStatusEntry ::=
    SEQUENCE {
        omniVoipChannelIndex           Integer32,
        omniVoipChannelName            DisplayString,
        omniVoipLinkUp                 Integer32,
        omniVoipABCDState              Integer32,
        omniVoipTransmitPackets        Counter32,
        omniVoipRxPackets              Counter32,
        omniVoipRxSequenceErrorPackets Counter32,
        omniVoipRxTooLatePackets       Counter32,
        omniRadioChannelNumber         Integer32
    }

omniVoipChannelIndex OBJECT-TYPE
    SYNTAX      Integer32(0..3)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Index for OmniVoip table"
    ::= { omniVoipStatusEntry 1 }

omniVoipChannelName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Name / SDES Location of channel "
    ::= { omniVoipStatusEntry 2 }

omniVoipLinkUp OBJECT-TYPE
    SYNTAX      Integer32(0..255)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Current status of the VoIP link. 0=Offline, 1=online, 255=Disabled "
    ::= { omniVoipStatusEntry 3 }

omniVoipABCDState OBJECT-TYPE
    SYNTAX      Integer32(0..16)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Current received Telephone ABCD bits (includes remote busy state)"
    ::= { omniVoipStatusEntry 4 }

omniVoipTransmitPackets OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Number of transmitted RTP packets since start-up"
    ::= { omniVoipStatusEntry 5 }

omniVoipRxPackets OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Number of received RTP packets since start-up"
    ::= { omniVoipStatusEntry 6 }

omniVoipRxSequenceErrorPackets OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Number of received RTP packets since start-up with incorrect sequence number, includes packets that have been lost"
    ::= { omniVoipStatusEntry 7 }

omniVoipRxTooLatePackets OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Number of received RTP packets since startup that have been received too late to be played"
    ::= { omniVoipStatusEntry 8 }

omniRadioChannelNumber OBJECT-TYPE
    SYNTAX      Integer32(1..899)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "Shows the current radio channel and can be used to trigger a channel change request."
    ::= { omniVoipStatusEntry 9 }

---
---  Radio Information: omniRadio
---
omniRadioNumberOfChannels OBJECT-TYPE
    SYNTAX Integer32(0..128)
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "Number of Radio Channels supported by this device"
        ::= { omniRadio 1 }

omniRadioStatusTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF OmniRadioStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table contains the status of the Radio links for this
        including Omnitronics specifics"
    ::=  { omniRadio 2 }

omniRadioStatusEntry OBJECT-TYPE
    SYNTAX      OmniRadioStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Row describing the status of a Radio connection"
    INDEX   { omniRadioChannelIndex }
    ::= { omniRadioStatusTable 1 }

OmniRadioStatusEntry ::= SEQUENCE {
    omniRadioChannelIndex            Integer32,
    omniRadioChannelName             DisplayString,
    omniRadioBusy                    Integer32,
    omniRadioPushToTalk              Integer32,
    spectraLEDAlarm                  Integer32,
    spectraLEDLockDetect             Integer32,
    spectraLEDCTCSS                  Integer32,
    spectraLEDPTT                    Integer32,
    spectraLEDMute                   Integer32,
    spectraLEDPower                  Integer32,
    spectraAlarmLowForwardPower      Integer32,
    spectraAlarmHighReflectedPower   Integer32,
    spectraAlarmLowInputVoltage      Integer32,
    spectraAlarmHighInputVoltage     Integer32,
    spectraAlarmTransmitterOutOfLock Integer32,
    spectraAlarmReceiverOutOfLock    Integer32,
    spectraAlarmPAOverTemperature    Integer32,
    spectraOpenCollectorPolarity     Integer32
}

omniRadioChannelIndex OBJECT-TYPE
    SYNTAX      Integer32(0..3)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Index for OmniRadio table"
    ::= { omniRadioStatusEntry 1 }

omniRadioChannelName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Name of radio channel"
    ::= { omniRadioStatusEntry 2 }

omniRadioBusy OBJECT-TYPE
    SYNTAX      Integer32(0..1)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Current radio busy (input) state. 0 is Off, 1 is On"
    ::= { omniRadioStatusEntry 3 }

omniRadioPushToTalk OBJECT-TYPE
    SYNTAX      Integer32(0..1)
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "Current radio push to talk (output) state. 0 is Off, 1 is On"
    ::= { omniRadioStatusEntry 4 }

---
---  Linker Information: omniLinker
---
omniLinkerVoipTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF OmniLinkerVoipEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table shows the current linker configuration between VOIP and radio channels.
        It also allows the audio, of radios linked to a VOIP channel, to be bridged."
    ::=  { omniLinker 1 }

omniLinkerVoipEntry OBJECT-TYPE
    SYNTAX      OmniLinkerVoipEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Row describing the linker configuration between VOIP and radio channels."
    INDEX   { omniLinkerVoipChannelIndex }
    ::= { omniLinkerVoipTable 1 }

OmniLinkerVoipEntry ::= SEQUENCE {
    omniLinkerVoipChannelIndex  Integer32,
    omniLinkerVoipChannelName   DisplayString,
    omniLinkerRadioChannel1     Integer32,
    omniLinkerRadioChannel2     Integer32,
    omniLinkerRadioChannel3     Integer32,
    omniLinkerRadioChannel4     Integer32,
    omniLinkerAudioBridge       Integer32
}

omniLinkerVoipChannelIndex OBJECT-TYPE
    SYNTAX      Integer32(0..128)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Index of Linker VOIP table and also the index of the VOIP channel"
    ::= { omniLinkerVoipEntry 1 }

omniLinkerVoipChannelName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Name / SDES Location of channel"
    ::= { omniLinkerVoipEntry 2 }

omniLinkerRadioChannel1 OBJECT-TYPE
    SYNTAX      Integer32(0..1)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Indicates if radio channel 1 is linked to the VOIP channel (see VOIP channel name for that row),
        where 1 is linked and 0 is not linked."
    ::= { omniLinkerVoipEntry 3 }

omniLinkerRadioChannel2 OBJECT-TYPE
    SYNTAX      Integer32(0..1)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Indicates if radio channel 2 is linked to the VOIP channel (see VOIP channel name for that row),
        where 1 is linked and 0 is not linked."
    ::= { omniLinkerVoipEntry 4 }

omniLinkerRadioChannel3 OBJECT-TYPE
    SYNTAX      Integer32(0..1)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Indicates if radio channel 3 is linked to the VOIP channel (see VOIP channel name for that row),
        where 1 is linked and 0 is not linked."
    ::= { omniLinkerVoipEntry 5 }

omniLinkerRadioChannel4 OBJECT-TYPE
    SYNTAX      Integer32(0..1)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Indicates if radio channel 4 is linked to the VOIP channel (see VOIP channel name for that row),
        where 1 is linked and 0 is not linked."
    ::= { omniLinkerVoipEntry 6 }

omniLinkerAudioBridge OBJECT-TYPE
    SYNTAX      Integer32(0..1)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Indicates whether all radio channels linked to the VOIP channel are bridged,
        where 1 is bridged and 0 implies the audio is not bridged."
    ::= { omniLinkerVoipEntry 7 }

omniLinkerAudioBridgeTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF OmniLinkerAudioBridgeEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table allows audio to be bridged between radios without VOIP."
    ::=  { omniLinker 2 }

omniLinkerAudioBridgeEntry OBJECT-TYPE
    SYNTAX      OmniLinkerAudioBridgeEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Row that describes how each radio's audio is bridged."
    INDEX   { omniLinkerRadioChannelIndex }
    ::= { omniLinkerAudioBridgeTable 1 }

OmniLinkerAudioBridgeEntry ::= SEQUENCE {
    omniLinkerRadioChannelIndex Integer32,
    omniLinkerRadioChannelName  DisplayString,
    omniLinkerLocalGroup1       Integer32,
    omniLinkerLocalGroup2       Integer32
}

omniLinkerRadioChannelIndex OBJECT-TYPE
    SYNTAX      Integer32(0..3)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is the index of the Linker Audio Bridge table and also the index of the radio channel."
    ::= { omniLinkerAudioBridgeEntry 1 }

omniLinkerRadioChannelName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Name of radio channel"
    ::= { omniLinkerAudioBridgeEntry 2 }

omniLinkerLocalGroup1 OBJECT-TYPE
    SYNTAX      Integer32(0..1)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Two groups are provided to allow the user to specify how each radio's audio is bridged.
        All radios selected (by giving a value of 1) in a particular group will have their audio bridged.
        A radio can be removed from a group by entering 0."
    ::= { omniLinkerAudioBridgeEntry 3 }

omniLinkerLocalGroup2 OBJECT-TYPE
    SYNTAX      Integer32(0..1)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Two groups are provided to allow the user to specify how each radio's audio is bridged.
        All radios selected (by giving a value of 1) in a particular group will have their audio bridged.
        A radio can be removed from a group by entering 0."
    ::= { omniLinkerAudioBridgeEntry 4 }

